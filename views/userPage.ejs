<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>user page</title>
</head>
<body>
  
    <a href="/logout">Logout</a>
    <ul>
        <% user.forEach((u) => { %>
           <li>Id: <%= u.id %></li>
           <li>Name: <%= u.name %></li>
           <li>Password: <%= u.password %></li>
             <a href="/update/<%= u.id %>">Edit Your Account </a> |
             <a href="/dtl/<%= u.id %>"  onclick="return confirm('Are You Sure You Want to delete your account??')">Delete</a> 
        <% }) %>
    </ul>
  
</body>
</html>